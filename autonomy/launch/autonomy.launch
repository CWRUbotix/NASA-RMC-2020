<launch>
	<node name="transit_node" pkg="autonomy" type="transit_node.py" launch-prefix="xterm -e"/>
	<node name="autonomy_node" pkg="autonomy" type="autonomy_node.py" launch-prefix="xterm -e"/>
	<node name="robot_state_node" pkg="autonomy" type="robot_state.py" output="screen"/>
	<node name="simulator_node" pkg="autonomy" type="simulator.py" unless="$(eval '192.168.0.100' in optenv('ROS_MASTER_URI'))"/>
</launch>
