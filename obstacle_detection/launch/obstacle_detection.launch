<launch>
    <rosparam command="load" file = "$(find obstacle_detection)/params/obstacle_detection_params.yaml" ns="obstacle_detection"/>

	<node name="obstacle_detection_node" pkg="obstacle_detection" type="obstacle_detection_node.py" cwd="node"/>
	<node name="global_occupancy_grid_node" pkg="obstacle_detection" type="global_occupancy_grid.py" output="screen" cwd="node"/>
	<node name="realsense_motion_tracking" pkg="obstacle_detection" type="realsense_motion_tracking.py" cwd="node"/>
</launch>
